[diskusage]
interval=7200
command=~/.local/bin/sbar/sb-diskusage

[network]
interval=7200
command=~/.local/bin/sbar/sb-net

[volume]
interval=once
signal=3
command=~/.local/bin/sbar/sb-volume

[updates]
interval=3600
command=~/.local/bin/sbar/sb-updates

[weather]
interval=3600
command=~/.local/bin/sbar/sb-weather

[cputemp]
interval=60
command=~/.local/bin/sbar/sb-cputemp

[date_time]
interval=30
command=~/.local/bin/sbar/sb-date
